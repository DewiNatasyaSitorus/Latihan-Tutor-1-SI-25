<?xml version="1.0"?>
<flowgorithm fileversion="4.2">
    <attributes>
        <attribute name="name" value="T00"/>
        <attribute name="authors" value="dewi"/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2025-10-01 09.33.44 PM"/>
        <attribute name="created" value="ZGV3aTtNU0k7MjAyNS0xMC0wMTswOC4zMC40NiBQTTsxOTEw"/>
        <attribute name="edited" value="ZGV3aTtNU0k7MjAyNS0xMC0wMTswOS4zMy40NCBQTTs1OzIwMjQ="/>
    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="i1, i2, j, k" type="Integer" array="False" size=""/>
            <input variable="i1"/>
            <input variable="i2"/>
            <if expression="i1%2=0">
                <then>
                    <output expression="&quot;Bilangan pertama:&quot;&amp;&quot;Genap&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;Bilangan pertama:&quot;&amp;&quot;Ganjil&quot;" newline="True"/>
                </else>
            </if>
            <if expression="i2%2=0">
                <then>
                    <output expression="&quot;Bilangan kedua:&quot;&amp;&quot;Genap&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;Bilangan kedua:&quot;&amp;&quot;Genap&quot;" newline="True"/>
                </else>
            </if>
            <if expression="i1&gt;i2">
                <then>
                    <output expression="&quot;Bilangan pertama lebih besar&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="i2&gt;i1">
                        <then>
                            <output expression="&quot;Bilangan kedua lebih besar&quot;" newline="True"/>
                        </then>
                        <else>
                            <output expression="&quot;Bilangan sama besar&quot;" newline="True"/>
                        </else>
                    </if>
                </else>
            </if>
            <if expression="i1%2=0 &amp;&amp; i2%2=0">
                <then>
                    <assign variable="j" expression="i1+i2"/>
                    <output expression="&quot;Hasil penjumlahan: &quot; &amp; j" newline="True"/>
                </then>
                <else>
                    <if expression="i1%2=1 &amp;&amp; i2%2=1">
                        <then>
                            <assign variable="k" expression="i1*i2"/>
                            <output expression="&quot;Hasil perkalian: &quot; &amp; k" newline="True"/>
                        </then>
                        <else>
                            <output expression="&quot;Berbeda jenis&quot;" newline="True"/>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
</flowgorithm>
